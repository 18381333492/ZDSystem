                            List<MRolePermission> FirstMenus = ModuleList.Where(u => u.FunctionPid == item.FunctionID && u.FunctionLevel == 3).ToList();
                            if (FirstMenus == null || FirstMenus.Count == 0)
                            {
                                continue;
                            }
                        <li>
                            <img src='@string.Format("../../Content/Images/{0}", item.IconPath)' />
